{
  "name": "rental-investment-analyzer-chrome",
  "version": "0.1.0",
  "description": "Chrome extension to analyze rental investment properties on Zillow",
  "main": "src/background/background.ts",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,json,css,html}\"",
    "generate-icons": "node scripts/generate-icons.js",
    "convert-hud-data": "node scripts/convert-hud-data.js",
    "prebuild": "npm run generate-icons",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "extract-samples": "tsx tests/extractSampleData.ts"
  },
  "keywords": [
    "chrome-extension",
    "real-estate",
    "investment",
    "rental",
    "zillow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/chrome": "^0.0.260",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^30.0.0-beta.3",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sharp": "^0.33.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "xlsx": "^0.18.5"
  }
}
