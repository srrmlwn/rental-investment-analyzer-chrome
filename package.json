{
  "name": "rental-investment-analyzer-chrome",
  "version": "0.1.0",
  "description": "Chrome extension to analyze rental investment properties on Zillow",
  "main": "src/background/background.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,json,css,html}\"",
    "generate-icons": "node scripts/generate-icons.js",
    "convert-hud-data": "node scripts/convert-hud-data.js",
    "prebuild": "npm run generate-icons && npm run convert-hud-data"
  },
  "keywords": [
    "chrome-extension",
    "real-estate",
    "investment",
    "rental",
    "zillow"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "sharp": "^0.33.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "xlsx": "^0.18.5"
  }
} 